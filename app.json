{
  "expo": {
    "name": "Medbikri-SpaceX-APP",
    "slug": "Medbikri-SpaceX-APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "SpaceX-APP is the react native app which shows the list of launchpads and launches using the SpaceX-API.",
    "githubUrl": "https://github.com/ShashwatNautiyal/react-native-spaceX-app"
  }
}
